{
  "name": "poc_prisma",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "ts-node --transpile-only ./src/index.ts",
    "dev": "ts-node-dev --transpile-only ./src/index.ts",
    "start:grpc": "ts-node --transpile-only ./src/grpc/index.ts",
    "dev:grpc": "ts-node-dev --transpile-only ./src/grpc/index.ts",
    "build": "tsc",
    "clean": "rm -rf dist",
    "lint": "npm run lint:tsc && npm run lint:eslint",
    "lint:eslint": "eslint --ext .js,.jsx,.ts,.tsx ./",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx ./ --fix",
    "lint:tsc": "tsc --noEmit",
    "grpc:typesgen": "npx proto-loader-gen-types --keep-case --longs=String --enums=String --defaults --oneofs --grpcLib=@grpc/grpc-js --outDir=src/grpc/generated/ src/grpc/proto/*.proto"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/google-protobuf": "^3.15.2",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/koa": "^2.13.3",
    "@types/koa-pino-logger": "^3.0.0",
    "@types/koa__router": "^8.0.4",
    "@types/node": "^14.17.3",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "pino-pretty": "^5.0.2",
    "prettier": "^2.3.1",
    "prisma": "^2.25.0",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.3.4"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.3.2",
    "@grpc/proto-loader": "^0.6.2",
    "@koa/router": "^10.0.0",
    "@prisma/client": "^2.25.0",
    "apollo-server-koa": "^2.25.1",
    "env-var": "^7.0.1",
    "google-protobuf": "^3.17.3",
    "graphql": "^15.5.0",
    "graphql-iso-date": "^3.6.1",
    "koa": "^2.13.1",
    "koa-helmet": "^6.1.0",
    "koa-pino-logger": "^3.0.0",
    "nexus": "^1.0.0"
  }
}
